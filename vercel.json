{
    "version": 2,
    "builds": [
      { "src": "src/server.js", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  